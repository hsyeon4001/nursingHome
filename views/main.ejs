	<script>
		var strArr = ["images/cat1.jpg", "images/cat2.jpg", "images/cat3.jpg"];
		var img = document.querySelector(".main__img-section--img > img");
		var firstDot = document.querySelector(
			".main__img-section--circles > div:first-child"
		);
		var secondDot = document.querySelector(
			".main__img-section--circles > div:nth-child(2)"
		);
		var thirdDot = document.querySelector(
			".main__img-section--circles > div:last-child"
		);
		var dots = [firstDot, secondDot, thirdDot];
		function changeImg() {
			var strIdx = Math.round(Math.random() * 2);
			console.log(strIdx);
			img.src = strArr[strIdx];
		}
		function hoverImg(dot) {
			console.log("here");
			img.src = strArr[dots.indexOf(dot)];
		}

		setInterval(changeImg, 3000);

		function init() {
			img.addEventListener("onload", changeImg);
			firstDot.addEventListener("mouseover", console.log("first!"));
		}

		// circle hover 미해결
		init();
	</script>
